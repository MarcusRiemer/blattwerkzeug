{% if table %}
  <table class="table">
    <tr>
      {% for cell in table[0] %}
        <th>{{ cell[0] }}</th>
      {% endfor %}
    </tr>
    {% for row in table %}
      <tr>
        {% for cell in row %}
          <td>{{ cell[1] }}</td>
        {% endfor %}
      </tr>
    {% endfor %}
  </table>
{% else %}
  <div class="alert alert-danger" role="alert">
    Diese Tabelle kann nicht angezeigt werden, weil ihr <strong>keine Abfrage</strong> Ã¼bergeben worden ist.
  </div>
{% endif %}
