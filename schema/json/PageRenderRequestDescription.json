    "properties": {
        "sourceType": {
            "type": "string"
        },
        "source": {
            "type": "string"
        },
        "queries": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "sql": {
                        "type": "string"
                    }
                },
                "additionalProperties": false
            }
        },
        "params": {
            "type": "object",
            "additionalProperties": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false,
    "$schema": "http://json-schema.org/draft-04/schema#"
}
